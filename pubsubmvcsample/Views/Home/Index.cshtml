@model Orders

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>



    <form asp-controller="Home" asp-action="Index" enctype="multipart/form-data">
        <div class="row">
            <div class="col-md-10">

                <div class="form-group">
                    <label asp-for="UserEmail"></label>
                    <input asp-for="UserEmail" class="form-control" />
                </div>

                <div class="form-group">
                    <label asp-for="PhotoUrl"></label>
                    <input asp-for="PhotoUrl" class="form-control" />
                </div>
            </div>
        </div>
        <input type="submit" class="btn btn-info" value="Submit" />
    </form>
</div>
